        /* FAQ Section */
        .faq {
            background: var(--light-grey);
            position: relative;
        }

        .faq::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            background: linear-gradient(135deg, transparent, rgba(14, 94, 164, 0.02), transparent);
            pointer-events: none;
        }

        .faq-header {
            text-align: center;
            margin-bottom: 4rem;
