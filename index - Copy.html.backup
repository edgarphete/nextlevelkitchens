            margin-bottom: 0;
        }

        .faq-answer-content ul {
            margin: 1rem 0;
            padding-left: 0;
            list-style: none;
        }

        .faq-answer-content li {
            margin-bottom: 0.75rem;
            padding-left: 1.5rem;
            position: relative;
        }

        .faq-answer-content li::before {
            content: '✓';
